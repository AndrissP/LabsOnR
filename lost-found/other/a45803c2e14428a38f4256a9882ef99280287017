#Pb
l_Pb=c(0,2,3)
N_Pb=c(2217,17,7)
ln_Pb=log(N_Pb/60)
svari_Pb=1/N_Sr*sqrt(N_Pb)
fit_Pb<-lm(ln_Pb~l_Pb,weights=1/svari_Pb^2)
plot(l_Pb,ln_Pb)
lines(l_Pb,fit_Pb$fitted.values)

#Terauds
l_Te=c(0,1,2)
N_Te=c(2217,8,11)
ln_Te=log(N_Te/60)
svari_Te=1/N_Te*sqrt(N_Te)
fit_Te<-lm(ln_Te~l_Te,weights=1/svari_Te^2)
plot(l_Te,ln_Te)
lines(l_Te,fit_Te$fitted.values)

#Aluminijs
l_Al=c(0,1,2,3)
