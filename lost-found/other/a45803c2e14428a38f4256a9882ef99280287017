#Papîrs
l_Pa=c(0,1,2,3,4,5)
N_Pa=c(2217,1519,1026,826,579,667)
ln_Pa=log(N_Pa/60)
svari_Pa=1/N_Pa*sqrt(N_Pa)
fit_Pa<-lm(ln_Pa~l_Pa,weights=1/svari_Pa^2)
plot(l_Pa,ln_Pa)
lines(l_Pa,fit_Pa$fitted.values)

mu=c(2.19 ,3.107, 1.59, 0.723, 0.574, 0.278) 
rho<-c(11.11, 7.68, 2.7, 1.39, 1.19, 0.8)
Dmu<-c(0.26, 1.08, 0.14, 0.045, 0.035, 0.036)
Rez=mu/rho
DRez=Dmu/rho

D_pr=0.693/Rez
E=0.546
D=0.693/mu
dD=0.693/mu^2*Dmu
