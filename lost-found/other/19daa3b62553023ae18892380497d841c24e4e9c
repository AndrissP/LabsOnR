n3<-c(1.373,0.883,0.01)
n4<-c(1.357,0.867,0.011)
n5<-c(1.352,0.862,0.013)
n6<-c(1.349,0.859,0.011)
tab<-data.frame(n3,n4,n5,n6)
E3<--R*h*c/(3-n3)^2/e
E4<--R*h*c/(4-n4)^2/e
E5<--R*h*c/(5-n5)^2/e
E6<--R*h*c/(6-n6)^2/e
En<-data.frame(E3,E4,E5,E6)
la1<-h*c/(En[2,2]-En[1,1])/e
la2<-h*c/(En[3,1]-En[2,1])/e